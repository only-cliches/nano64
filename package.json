{
  "name": "nano64",
  "version": "1.3.0",
  "description": "64-bit Time-Sortable Identifiers for TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Scott Lott <me@scottlott.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "minify": "npx terser dist/index.js --compress --mangle --output dist/index.min.js",
    "size": "gzip -c dist/index.min.js | wc -c"
  },
  "repository": {
    "url": "https://github.com/only-cliches/nano64"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "keywords": [
    "nano64",
    "ulid",
    "time-sortable",
    "64-bit id",
    "bigint",
    "aes-gcm",
    "uid",
    "uuid alternative",
    "distributed id",
    "database key",
    "monotonic id",
    "sortable id",
    "crypto id",
    "typescript",
    "nodejs",
    "browser",
    "timestamp id"
  ]
}
